{
  "name": "@sylphx/molt",
  "version": "0.1.0",
  "description": "Unified data transformation stack - JSON, XML, YAML, TOML, CSV with type preservation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "molt",
    "json",
    "xml",
    "yaml",
    "toml",
    "csv",
    "parser",
    "serializer",
    "transformer",
    "dirty-json",
    "type-preservation",
    "streaming",
    "fast",
    "performance",
    "sylphx"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylphx/molt.git",
    "directory": "packages/molt"
  },
  "bugs": {
    "url": "https://github.com/sylphx/molt/issues"
  },
  "homepage": "https://github.com/sylphx/molt/tree/main/packages/molt#readme",
  "dependencies": {
    "@sylphx/molt-json": "workspace:*",
    "@sylphx/molt-xml": "workspace:*",
    "@sylphx/molt-yaml": "workspace:*",
    "@sylphx/molt-toml": "workspace:*",
    "@sylphx/molt-csv": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.0"
  }
}
