{
  "name": "@sylphx/molt-msgpack",
  "version": "1.0.2",
  "description": "High-performance MessagePack binary format parser and serializer",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "msgpack",
    "messagepack",
    "binary",
    "serialization",
    "parser",
    "encoder",
    "decoder",
    "performance",
    "rpc"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/molt.git",
    "directory": "packages/msgpack"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/molt/issues"
  },
  "homepage": "https://github.com/SylphxAI/molt#readme",
  "devDependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "msgpack-lite": "^0.1.26",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^4.0.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
