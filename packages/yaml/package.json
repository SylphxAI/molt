{
  "name": "@sylphx/molt-yaml",
  "version": "1.0.2",
  "description": "YAML transformer - dirty cleaning, type preservation, and streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "bench": "vitest bench --run",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "yaml",
    "parser",
    "serializer",
    "dirty-yaml",
    "typed-yaml",
    "streaming",
    "fast",
    "performance",
    "molt",
    "sylphx"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/molt.git",
    "directory": "packages/yaml"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/molt/issues"
  },
  "homepage": "https://github.com/SylphxAI/molt#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^25.0.2",
    "@vitest/ui": "^4.0.15",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0"
  }
}
