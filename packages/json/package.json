{
  "name": "@sylphx/molt-json",
  "version": "1.0.1",
  "description": "The fastest JSON transformer - 380x faster with dirty cleaning, type preservation, and streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "wasm-pkg",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "bench": "vitest bench",
    "detect-platform": "bun run scripts/detect-platform.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "json",
    "parser",
    "serializer",
    "dirty-json",
    "typed-json",
    "streaming",
    "validation",
    "fast",
    "performance",
    "molt",
    "sylphx",
    "type-preservation",
    "ndjson"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/molt.git",
    "directory": "packages/json"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/molt/issues"
  },
  "homepage": "https://github.com/SylphxAI/molt#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "@vitest/ui": "^2.1.5",
    "dirty-json": "^0.9.2",
    "superjson": "^2.2.5",
    "tinybench": "^2.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "zod": "^4.1.12"
  },
  "engines": {
    "bun": ">=1.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
