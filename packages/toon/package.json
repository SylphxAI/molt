{
  "name": "@sylphx/molt-toon",
  "version": "1.0.1",
  "description": "Token-Oriented Object Notation (TOON) parser - Reduce LLM token usage by 30-60%",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "toon",
    "token-oriented",
    "llm",
    "ai",
    "parser",
    "serializer",
    "tokens",
    "chatgpt",
    "claude",
    "gpt",
    "prompt"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/molt.git",
    "directory": "packages/toon"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/molt/issues"
  },
  "homepage": "https://github.com/SylphxAI/molt#readme",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
