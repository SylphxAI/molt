{
  "name": "@sylphx/molt-csv",
  "version": "0.1.0",
  "description": "CSV transformer - handle CSV with type preservation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "wasm-pkg"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "bench": "vitest bench --run"
  },
  "keywords": [
    "csv",
    "parser",
    "serializer",
    "transformer",
    "molt",
    "tsv"
  ],
  "author": "sylphx",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
