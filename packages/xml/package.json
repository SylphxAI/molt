{
  "name": "@sylphx/molt-xml",
  "version": "1.0.0",
  "description": "XML transformer - dirty cleaning, type preservation, and streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "bench": "vitest bench --run",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "xml",
    "parser",
    "serializer",
    "dirty-xml",
    "typed-xml",
    "streaming",
    "fast",
    "performance",
    "molt",
    "sylphx"
  ],
  "author": "Sylphx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylphx/molt.git",
    "directory": "packages/xml"
  },
  "bugs": {
    "url": "https://github.com/sylphx/molt/issues"
  },
  "homepage": "https://github.com/sylphx/molt/tree/main/packages/xml#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "@vitest/ui": "^2.1.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "engines": {
    "bun": ">=1.0"
  }
}
