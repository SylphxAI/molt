{
  "name": "@sylphx/molt",
  "version": "0.1.0",
  "private": true,
  "description": "High-performance data transformation stack for JSON, XML, YAML and more",
  "type": "module",
  "workspaces": ["packages/*"],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "bench": "turbo bench",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "turbo typecheck",
    "clean": "rm -rf packages/*/dist packages/*/node_modules node_modules",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "turbo build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.7",
    "@iarna/toml": "^2.2.5",
    "@types/node": "^22.9.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "d3-dsv": "^3.0.1",
    "fast-xml-parser": "^5.3.1",
    "js-yaml": "^4.1.1",
    "papaparse": "^5.5.3",
    "sharp": "^0.34.5",
    "smol-toml": "^1.4.2",
    "turbo": "^2.6.1",
    "typescript": "^5.6.3",
    "vitepress": "^1.6.4",
    "xml2js": "^0.6.2",
    "yaml": "^2.8.1"
  },
  "packageManager": "bun@1.3.2",
  "engines": {
    "bun": ">=1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylphx/molt.git"
  },
  "bugs": {
    "url": "https://github.com/sylphx/molt/issues"
  },
  "homepage": "https://github.com/sylphx/molt#readme",
  "author": "Sylphx",
  "license": "MIT"
}
